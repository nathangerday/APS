[
  CONST xs (vec (vec int)) (alloc 3);
  SET (nth xs 0) (alloc 2);
  SET (nth xs 1) (alloc 3);
  SET (nth xs 2) (alloc 4);
  SET (nth (nth xs 0) 0) 1;
  SET (nth (nth xs 0) 1) 1;
  SET (nth (nth xs 1) 0) 2;
  SET (nth (nth xs 1) 1) 2;
  SET (nth (nth xs 1) 2) 2;
  SET (nth (nth xs 2) 0) 1;
  SET (nth (nth xs 2) 1) 1;
  SET (nth (nth xs 2) 2) 1;
  SET (nth (nth xs 2) 3) 1;
  ECHO (nth (nth xs 0) 0)
]
